{
    "name": "@mcp-servers/google-ads",
    "version": "1.0.0",
    "description": "MCP server for Google Ads API operations",
    "main": "dist/index.js",
    "type": "module",
    "scripts": {
        "build": "tsc",
        "start": "node dist/index.js",
        "start:stdio": "node dist/index.js --transport=stdio",
        "start:http": "node dist/index.js --transport=http",
        "dev": "tsx src/index.ts",
        "dev:http": "tsx src/index.ts --transport=http"
    },
    "keywords": [
        "mcp",
        "google-ads",
        "google-cloud",
        "advertising",
        "model-context-protocol"
    ],
    "dependencies": {
        "@mcp-servers/shared": "file:../shared",
        "@modelcontextprotocol/sdk": "^1.12.1",
        "dotenv": "^16.5.0",
        "google-ads-api": "^20.0.1"
    },
    "devDependencies": {
        "@types/node": "^20.10.0",
        "tsx": "^4.6.0",
        "typescript": "^5.3.0"
    }
}
