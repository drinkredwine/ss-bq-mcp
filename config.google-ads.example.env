# Google Ads API Configuration
# Copy this file to .env and fill in your actual values

# Your Google Ads Customer ID (without dashes)
GOOGLE_ADS_CUSTOMER_ID=1234567890

# Your Google Ads API Developer Token
GOOGLE_ADS_DEVELOPER_TOKEN=your-developer-token-here

# OAuth2 Client Credentials
GOOGLE_ADS_CLIENT_ID=your-client-id.apps.googleusercontent.com
GOOGLE_ADS_CLIENT_SECRET=your-client-secret

# OAuth2 Refresh Token (generated through OAuth2 flow)
GOOGLE_ADS_REFRESH_TOKEN=your-refresh-token

# Optional: Default location for Google Cloud services
GOOGLE_CLOUD_LOCATION=US 